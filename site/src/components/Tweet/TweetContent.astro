---
import { getTweet } from './api'
import EmbeddedTweet from './twitter-theme/EmbeddedTweet.astro'
interface Props {
	id: string
	autoplay?: boolean
	fetchOptions?: RequestInit
	onError?: ((error: any) => any) | undefined
}

const { id, fetchOptions, autoplay } = Astro.props
const tweet = await getTweet(id, fetchOptions)
---

<EmbeddedTweet {autoplay} tweet={tweet} />
